# PDF.co API Key
pdf_extractor_api_key = "YOUR_PDFCO_API_KEY"

[modelConfig]
# OpenAI API Key
openAiToken = "YOUR_OPENAI_API_KEY"
model = "gpt-4-turbo"

[databaseConfig]
host = "localhost"
port = 3306
user = "root"
password = "YOUR_DB_PASSWORD"
database = "pixel"

[corsConfig]
allowOrigins = ["http://localhost:5173"]
allowCredentials = false
allowHeaders = ["authorization", "content-type", "x-requested-with"]
allowMethods = ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
exposeHeaders = ["content-type"]
maxAge = 86400

[jwtConfig]
issuer = "pixel"
audience = "pixelAi-users"
expTime = 86400
privateKeyPath = "./resources/private.key"
clockSkew = 60
certFile = "./resources/cert.pem"